{
  "name": "video2day",
  "version": "2.6.0",
  "description": "Video/iframe generator for the Twoday blogging platform",
  "main": "video2day.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/NeonWilderness/video2day-ts.git"
  },
  "resolutions": {
    "natives": "1.1.3"
  },
  "scripts": {
    "build": "gulp --production",
    "dev": "gulp",
    "copy": "gulp copy",
    "lint": "eslint . --quiet --ext .ts",
    "scripts": "node ./util/uploadScripts",
    "stories": "node ./util/updateStories",
    "testpage": "node ./node_modules/local-web-server/bin/cli --spa ./dist/testpage.html",
    "teststory": "node ./node_modules/local-web-server/bin/cli --spa ./dist/teststory.html",
    "testtool": "node ./node_modules/local-web-server/bin/cli --spa ./dist/testtool.html"
  },
  "keywords": [
    "video generator",
    "iframe generator",
    "Twoday"
  ],
  "author": "NeonWilderness",
  "license": "MIT",
  "dependencies": {
    "load-script2": "^2.0.6",
    "lozad": "^1.16.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/jquery": "^1.10.36",
    "@types/knockout": "^3.4.69",
    "@types/lozad": "^1.9.0",
    "@types/mocha": "^8.2.0",
    "@types/request-promise": "^4.1.47",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "cheerio": "^1.0.0-rc.3",
    "css-loader": "^5.0.1",
    "del": "^6.0.0",
    "dotenv-safe": "^8.2.0",
    "eslint": "^7.16.0",
    "gulp": "^4.0.2",
    "gulp-pug": "^4.0.1",
    "less": "^4.0.0",
    "less-loader": "^7.1.0",
    "local-web-server": "^4.2.1",
    "mocha": "^8.2.1",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "smoothscroll": "^0.4.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.12",
    "typescript": "^4.1.3",
    "webpack": "^4.44.2",
    "yargs": "^16.2.0"
  }
}
