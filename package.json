{
  "name": "video2day",
  "version": "2.4.1",
  "description": "Video/iframe generator for the Twoday blogging platform",
  "main": "video2day.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/NeonWilderness/video2day-ts.git"
  },
  "resolutions": {
    "natives": "1.1.3"
  },
  "scripts": {
    "build": "gulp --production",
    "dev": "gulp",
    "deploy": "gulp deploy --production",
    "deploy:blog": "gulp copy",
