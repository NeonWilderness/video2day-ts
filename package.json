    "@types/jquery": "^3.3.10",
    "@types/knockout": "^3.4.59",
    "@types/lozad": "^1.1.0",
    "@types/mocha": "^5.2.5",
    "@types/request-promise": "^4.1.42",
    "chai": "^4.2.0",
    "chalk": "^2.4.1",
