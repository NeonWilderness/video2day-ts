{
  "name": "video2day",
  "version": "2.7.0",
  "description": "Video/iframe generator for the Twoday blogging platform",
  "main": "video2day.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/NeonWilderness/video2day-ts.git"
  },
  "resolutions": {
    "natives": "1.1.3"
  },
  "scripts": {
    "build": "gulp --production",
    "dev": "gulp",
    "copy": "gulp copy",
    "lint": "eslint . --quiet --ext .ts",
    "scripts": "node ./util/uploadScripts",
    "stories": "node ./util/updateStories",
    "testpage": "node ./node_modules/local-web-server/bin/cli --spa ./dist/testpage.html",
    "teststory": "node ./node_modules/local-web-server/bin/cli --spa ./dist/teststory.html",
    "testtool": "node ./node_modules/local-web-server/bin/cli --spa ./dist/testtool.html"
  },
  "keywords": [
    "video generator",
    "iframe generator",
    "Twoday"
  ],
  "author": "NeonWilderness",
  "license": "MIT",
  "dependencies": {
    "load-script2": "^2.0.6",
    "lozad": "^1.16.0"
  },
  "devDependencies": {
    "@neonwilderness/twoday": "^0.3.14",
    "@types/chai": "^4.2.22",
    "@types/jquery": "^1.10.36",
    "@types/knockout": "^3.4.71",
    "@types/lozad": "^1.16.1",
    "@types/mocha": "^9.0.0",
    "@types/request-promise": "^4.1.48",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "chai": "^4.3.4",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.10",
    "css-loader": "^6.4.0",
    "del": "^6.0.0",
    "dotenv-safe": "^8.2.0",
    "eslint": "^7.32.0",
    "gulp": "^4.0.2",
    "gulp-pug": "^5.0.0",
    "less": "^4.1.2",
    "less-loader": "^10.1.0",
    "local-web-server": "^5.1.1",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "style-loader": "^3.3.0",
    "terser-webpack-plugin": "^5.2.4",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.58.2",
    "yargs": "^17.2.1"
  }
}
